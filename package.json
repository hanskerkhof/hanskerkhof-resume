{
  "name": "hanskerkhof",
  "version": "4.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build",
    "test": "jest --coverage --maxWorkers=4",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run hanskerkhof:serve-ssr",
    "serve:ssr": "node dist/hanskerkhof/server/main.js",
    "build:ssr": "ng build --configuration production && ng run hanskerkhof:server:production",
    "prerender": "ng run hanskerkhof:prerender --routesFile=routes.txt",
    "deploy-aws-s3": "scripts/deploy-aws-s3.sh",
    "deploy-gh-pages": "ng deploy --no-silent --name='CI/CD' --email='info@netmatic.nl' --message='deploy-gh-pages [skip ci]\n\nSkip this commit on CircleCi '",
    "all-tasks": "run-p lint test prerender"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~12.2.2",
    "@angular/common": "~12.2.2",
    "@angular/compiler": "~12.2.2",
    "@angular/core": "~12.2.2",
    "@angular/forms": "~12.2.2",
    "@angular/platform-browser": "~12.2.2",
    "@angular/platform-browser-dynamic": "~12.2.2",
    "@angular/platform-server": "~12.2.2",
    "@angular/router": "~12.2.2",
    "@nguniversal/express-engine": "^12.0.2",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "bootstrap": "^4.5.0",
    "express": "^4.15.2",
    "font-awesome": "^4.7.0",
    "generate": "^0.14.0",
    "moment": "^2.27.0",
    "ngx-google-analytics": "^9.0.0",
    "ngx-meta-tags": "^0.2.1",
    "ngx-moment": "^4.0.1",
    "resume-schema": "^0.1.3",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.2",
    "@angular/cli": "~12.2.2",
    "@angular/compiler-cli": "~12.2.2",
    "@nguniversal/builders": "^12.0.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^26.0.23",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^1.0.0-rc.2",
    "codelyzer": "^6.0.0",
    "jest": "^27.0.4",
    "jest-preset-angular": "^9.0.4",
    "npm-run-all": "^4.1.5",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.2.4"
  }
}
